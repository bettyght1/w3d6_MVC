<%- include('./template.ejs') %>
<body>
        <div><a href='/shoppingCart'>View products in Cart</a></div>
        <div><a href='/'>Add more Item</a></div>
    <table>
        <tr>
            <th>Name</th>
            <th>Quantity</th>
            <th>Price</th>
        </tr>
        
        <% for(let item of itemList){ %>
        <tr>           
            <td><%= item.name %></td>
            <td><%= item.quantity %></td>
            <td><%= item.price %></td>
        </tr>
        <% } %> 
